<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="keywords" content="backend, servicio, notebook, pc, instalacion, w10, office, programación, c#, csharp, asp.net, appweb " />
    <meta name="description" content="En esta página, encontrarás servicios de
    mantenimiento y actualización para tu notebook o computadora de escritorio.
    Además, también ofrezco servicios de desarrollo de aplicaciones web y de
    escritorio. En diferentes secciones, podrás ver trabajos y proyectos
    que he realizado con mis conocimientos." />
    <title>@ViewData["Title"] - MuoqaWeb</title>
    <link rel="stylesheet" href="~/css/MuoqaFonts.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/MuoqaWeb.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
                <div class="black__background">
                    <div class="container-fluid">
                        <a class="navbar__title navbar-brand text-white" asp-area="" asp-controller="Home" asp-action="Index">MuoqaWeb</a>
                        @* <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                                aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button> *@
                    <div class="responsive__btn__containter">
                            <button class="responsive__btn" id="responsiveBtn">
                                <span class="responsive__img text-white">
                                    Menu
                                </span>
                            </button>
                        </div>
                    <div class="navbar__options" id="responsiveCollapse"> @* navbar-collapse collapse d-sm-inline-flex justify-content-between *@
                            <ul class="navbar-nav flex-grow-1">
                                <li class="nav-item">
                                    <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">Inicio</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="">Proximamente mas</a>
                                </li>
                            </ul>
                            @await Html.PartialAsync("_LoginPartial")
                            @*<partial name="_LoginPartial" />*@
                        </div>
                    </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - MuoqaWeb - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            <a target="_blank" href="https:"></a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
