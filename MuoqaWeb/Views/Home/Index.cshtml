@using System.Data
@{
    ViewData["Title"] = "Inicio";
    DataTable service = new DataTable();
    if (ViewBag.Data != null)
        service = ViewBag.Data;

    int limit = 0;
    //Calcular par de filas en service
    if(service.Rows.Count%2==0)
    {
        //Obtenemos la mitad del numero
        limit = service.Rows.Count / 2;
    }
    else
    {
        int aux = service.Rows.Count / 2;
        limit = aux +1;
        Console.WriteLine($"Index: {aux} Y {limit} Y {service.Rows.Count}");
    }
    Console.WriteLine($"Index: {limit.ToString()}");
}
<link rel="stylesheet" href="~/css/indexstyle.css" asp-append-version="true"/>
<div class="text-center">
    <section class="sections__works__all sections_works">
        <div class="works__part works__part1">
            <div class="view__office">
                <div class="filter filter3 black__background">
                    <p class="textInfo text-white">W10 + Office</p>
                    <p class="decription__service text-white">
                        Las instalaciones de Windows 10 y Windows 11 vienen con un paquete de aplicaciones comunes ya instaladas,
                        para que puedas usarlas lo antes posible y ahorrar tiempo.
                        Estas aplicaciones incluyen Google Chrome, VLC, WinRAR y mas. Además, también viene con el paquete de Office (Excel, Word, etc.).
                    </p>
                </div>
            </div>
        </div>
        <div class="works__part works__part2">
            <div class="service before__service">
